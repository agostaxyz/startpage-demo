---
import { Image } from 'astro:assets';

interface Props {
	title: string;
	href: string;
    img: string;
}
const { title, href, img } = Astro.props;
---
<a href={href}>
    <Image
        class="min-w-[54px] w-[54px] h-[54px] rounded-full my-4 mx-2 md:mx-0"
        src={img}
        alt={title}
        width={100}
        height={100}
        decoding="async"
        loading="lazy"
    ></Image>


</a>